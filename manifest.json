{
  "manifest_version": 2,
  "version": "1.0.2",
  "version_name": "1.0.2",
  "name": "Whatsapp Unofficial Dark Mode",
  "description": "This extension is just an unofficial support that allows you to enable the dark mode theme on Whatsapp Web.",
  "page_action": {
    "default_icon": "whatsapp-icon.png"
  },
  "icons": {
    "128": "whatsapp-dark-icon-128.png"
  },
  "background": {
    "scripts": [
      "main.js",
      "make-dark-mode.js"
    ],
    "persistent ": false
  },
  "permissions": [
    "tabs",
    "https://web.whatsapp.com/*"
  ]
}